{
  "name": "@tmp/editor",
  "private": true,
  "version": "0.0.1",
  "main": "dist/tmp-editor.umd.js",
  "module": "dist/tmp-editor.mjs",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tmp-editor.mjs",
      "require": "./dist/tmp-editor.umd.js"
